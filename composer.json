{
    "name": "nathan/age-test-micro",
    "description": "A an example application for a practical exercise.",
    "authors": [
        {
            "name": "Nathan Daly",
            "email": "justlikephp@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "slim/slim": "4.*",
        "slim/psr7": "^1.2",
        "nyholm/psr7": "^1.3",
        "nyholm/psr7-server": "^1.0",
        "guzzlehttp/psr7": "^1.7",
        "php-di/php-di": "^6.3",
        "http-interop/http-factory-guzzle": "^1.0",
        "laminas/laminas-diactoros": "^2.5",
        "vlucas/phpdotenv": "^5.2",
        "illuminate/database": "^8.16",
        "jenssegers/blade": "^1.4",
        "php-di/slim-bridge": "^3.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^9.3.10",
        "pestphp/pest": "^0.3.13"
    },
    "autoload": {
        "psr-4": {
            "Core\\": "core/",
            "App\\": "src/"
        }
    },
    "scripts": {
        "start": "php -S localhost:8000 -t public/",
        "test": "./vendor/bin/pest"
    }
}
